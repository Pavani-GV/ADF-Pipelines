{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorypav"
		},
		"linkservicedl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkservicedl'"
		},
		"ls_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql'"
		},
		"ls_onprime_source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_onprime_source'"
		},
		"linkgit_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"linkservicedl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagedatafactorypav.dfs.core.windows.net/"
		},
		"ls_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azuresqlserverpav.database.windows.net"
		},
		"ls_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azuresqlpav"
		},
		"ls_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin123"
		},
		"managertrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/9f275cba-4d7a-4c7e-af98-8dba8ee15d94/resourceGroups/azurefundamentals/providers/Microsoft.Storage/storageAccounts/storagedatafactorypav"
		},
		"ls_onprime_source_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\pavanifiles"
		},
		"ls_onprime_source_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "MIS"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
